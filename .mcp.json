      "type": "stdio"
    },
    "deepwiki": {
      "command": "node",
      "args": [
        "dist/index.js",
        "--upstream",
        "https://mcp.deepwiki.com/mcp",
        "--deny",
        "list_wiki_contents"
      ],
      "type": "stdio"
    }
  }
}
